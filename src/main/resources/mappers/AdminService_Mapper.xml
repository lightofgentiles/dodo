<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.myapp.persistence.AdminService_Mapper">

<select id="AdminProductSelectAll" parameterType="HashMap" resultType="pv">
select * from product where del_yn='N'
order by p_idx desc limit #{f_str},#{e_str}

</select>

<select id="AdminProductTotalCount" parameterType="scri" resultType="Integer">
select count(*) as cnt from product where del_yn='N'
</select>

</mapper>